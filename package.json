{
  "name": "@vizzuality/nestjs-base-service",
  "version": "0.1.0",
  "description": "An opinionated base service for NestJS apps",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/Vizzuality/nestjs-base-service.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --project .",
    "prepublishOnly": "tsc --project .",
    "lint": "eslint 'src/**/**.ts' --fix --ignore-pattern 'src/**/*.spec.ts'",
    "format": "prettier \"**/*.ts\" --ignore-path ./.prettierignore --write && git status"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,json}": [
      "prettier --ignore-path ./.prettierignore --write"
    ]
  },
  "peerDependencies": {},
  "optionalDependencies": {},
  "dependencies": {
    "@nestjs/common": "7.5.2",
    "class-validator": "0.12.2",
    "nestjs-typeorm-paginate": "2.2.1",
    "typeorm": "0.2.29",
    "typescript": "4.0.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.8.0",
    "@typescript-eslint/parser": "4.8.0",
    "@types/node": "14.14.7",
    "eslint": "7.10.0",
    "eslint-config-prettier": "6.15.0",
    "husky": "4.3.0",
    "jest": "26.6.3",
    "lint-staged": "10.5.1",
    "prettier": "2.1.2"
  },
  "author": {
    "name": "Vizzuality",
    "email": "hello@vizzuality.com"
  }
}
